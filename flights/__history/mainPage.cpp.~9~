//---------------------------------------------------------------------------

#include <fmx.h>
#pragma hdrstop

#include "mainPage.h"
#include "LoginPanel.h"
//---------------------------------------------------------------------------
#pragma package(smart_init)
#pragma resource "*.fmx"
TMainForm *MainForm;
//---------------------------------------------------------------------------
__fastcall TMainForm::TMainForm(TComponent* Owner)
	: TForm(Owner)
{
}
//---------------------------------------------------------------------------

void __fastcall TMainForm::Button2Click(TObject *Sender)
{
	LoginForm->Show();
	MainForm->Hide();
}
//---------------------------------------------------------------------------


void __fastcall TMainForm::homeUBtnClick(TObject *Sender)
{
	MainForm->Close();
	LoginForm->Show();
}
//---------------------------------------------------------------------------

